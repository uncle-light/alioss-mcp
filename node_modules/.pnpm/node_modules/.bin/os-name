#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chenhongguang/codes/alioss-fastmcp/node_modules/.pnpm/os-name@1.0.3/node_modules/os-name/node_modules:/Users/chenhongguang/codes/alioss-fastmcp/node_modules/.pnpm/os-name@1.0.3/node_modules:/Users/chenhongguang/codes/alioss-fastmcp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/chenhongguang/codes/alioss-fastmcp/node_modules/.pnpm/os-name@1.0.3/node_modules/os-name/node_modules:/Users/chenhongguang/codes/alioss-fastmcp/node_modules/.pnpm/os-name@1.0.3/node_modules:/Users/chenhongguang/codes/alioss-fastmcp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../os-name/cli.js" "$@"
else
  exec node  "$basedir/../os-name/cli.js" "$@"
fi
